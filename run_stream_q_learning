sbatch train stream_q.py --env_name="CliffwalkEnv" --debug  --seed=18950 --track=1 --tabular_env --use_sgd --lr=0.0001 --reward_rms=0 --layer_norm=0 --sparsity=0.0 --adaptive_step_size=0 --bound_delta=0 --eligibility_trace_lamda=0


